   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Submit a new job.\n",
      "\n",
      "Usage:\n",
      "runai submit [flags] -- [COMMAND] [args...] [options]\n",
      "Examples:\n",
      "\n",
      "# Start a Training job.\n",
      "runai submit --name train1 -i gcr.io/run-ai-demo/quickstart -g 1\n",
      "\n",
      "# Start an interactive job.\n",
      "runai submit --name build1 -i ubuntu -g 1 --interactive --attach\n",
      "\n",
      "# Use GPU Fractions\n",
      "runai submit --name frac05 -i gcr.io/run-ai-demo/quickstart -g 0.5\n",
      "\n",
      "# Hyperparameter Optimization\n",
      "runai submit --name hpo1 -i gcr.io/run-ai-demo/quickstart-hpo -g 1  \\\n",
      "    --parallelism 3 --completions 12 -v /nfs/john/hpo:/hpo\n",
      "\n",
      "# Auto generate job name\n",
      "runai submit -i gcr.io/run-ai-demo/quickstart -g 1\n",
      "\n",
      "\n",
      "Flags:\n",
      "\u001b[1mAliases/Shortcuts\u001b[0m\n",
      "      --inference                Mark this Job as inference.\n",
      "      --interactive              Mark this Job as interactive.\n",
      "      --job-name-prefix string   Set defined prefix for the job name and add index as suffix\n",
      "      --name string              Job name\n",
      "  -p, --project string           Specifies a project. Set a default project using 'runai config project <project name>'.\n",
      "      --template string          Load default values from a workload.\n",
      "\n",
      "\n",
      "\u001b[1mContainer Definition\u001b[0m\n",
      "      --attach                     If true, wait for the Pod to start running, and then attach to the Pod as if 'runai attach ...' were called. Attach makes tty and stdin true by default. Default false\n",
      "      --command                    If true, overrides the image's entrypoint with the command supplied after '--'.\n",
      "  -e, --environment stringArray    Set environment variables in the container.\n",
      "      --git-sync stringArray       Specifies git repositories to mount into the container. Use the format: source=REPO,branch=BRANCH_NAME,rev=REVISION,username=USER,password=PASSWORD,target=TARGET_DIRECTORY_TO_CLONE\n",
      "  -i, --image string               Container image to use when creating the job.\n",
      "      --image-pull-policy string   set image pull policy: always, ifNotPresent or never. (default \"Always\")\n",
      "      --run-as-user                Run in the context of the current CLI user rather than the root user.\n",
      "      --s3 stringArray             s3 storage details formatted as: bucket=BUCKET,key=KEY,secret=SECRET,url=URL,target=TARGET_PATH\n",
      "      --stdin                      Keep stdin open on the container(s) in the pod, even if nothing is attached.\n",
      "  -t, --tty                        Allocate a TTY for the container.\n",
      "      --working-dir string         Set the container's working directory.\n",
      "\n",
      "\n",
      "\u001b[1mResource Allocation\u001b[0m\n",
      "      --cpu string            CPU units to allocate for the job (0.5, 1)\n",
      "      --cpu-limit string      CPU limit for the job (0.5, 1)\n",
      "  -g, --gpu float             GPU units to allocate for the Job (0.5, 1).\n",
      "      --gpu-memory string     GPU Memory to allocate for this job (1G, 500M)\n",
      "      --large-shm             Mount a large /dev/shm device.\n",
      "      --memory string         CPU Memory to allocate for this job (1G, 20M)\n",
      "      --memory-limit string   Memory limit for this job (1G, 20M)\n",
      "      --mig-profile string    MIG profile to allocate for the job (1g.5gb, 2g.10gb, 3g.20gb, 4g.20gb, 7g.40gb)\n",
      "\n",
      "\n",
      "\u001b[1mStorage\u001b[0m\n",
      "      --mount-propagation    Enable HostToContainer mount propagation for all container volumes\n",
      "      --nfs-server string    Default NFS server for any volume which does not specify 'NfsHost'\n",
      "      --pvc stringArray      Mount a persistent volume. Syntax: 'StorageClass[optional]:Size:ContainerMountPath:ro[optional]'\n",
      "  -v, --volume stringArray   Volumes to mount into the container. Syntax: 'HostPath:ContainerPath:AccessMode[optional]:NfsHost[optional]'\n",
      "\n",
      "\n",
      "\u001b[1mNetwork\u001b[0m\n",
      "      --host-ipc           Use the host's ipc namespace.\n",
      "      --host-network       Use the host's network stack inside the container.\n",
      "      --port stringArray   Expose ports from the Job container.\n",
      "\n",
      "\n",
      "\u001b[1mJob Lifecycle\u001b[0m\n",
      "      --address string              Comma separated list of IP addresses to listen to when running with --service-type portforward (default: localhost)\n",
      "      --backoff-limit int           The number of times the job will be retried before failing. Default 6.\n",
      "      --completions int             Number of successful pods required for this job to be completed. Used with HPO.\n",
      "      --jupyter                     Run a Jupyter notebook using a default image and notebook configuration.\n",
      "      --parallelism int             Number of pods to run in parallel at any given time.  Used with HPO.\n",
      "      --preemptible                 Interactive preemptible jobs can be scheduled above guaranteed quota but may be reclaimed at any time.\n",
      "  -s, --service-type string         External access type to interactive jobs. Options are: portforward, loadbalancer, nodeport, ingress.\n",
      "      --ttl-after-finish duration   The duration, after which a finished job is automatically deleted (e.g. 5s, 2m, 3h).\n",
      "\n",
      "\n",
      "\u001b[1mAccess Control\u001b[0m\n",
      "      --allow-privilege-escalation     Allow the job to gain additional privileges after start.\n",
      "      --create-home-dir                Create a temporary home directory. Default is true when the --run-as-user flag is set, and false if not.\n",
      "      --prevent-privilege-escalation   privileges are prevented by default. use --allow-privilege-escalation for requesting additional privileges for the job\n",
      "\n",
      "\n",
      "\u001b[1mScheduling\u001b[0m\n",
      "      --node-pools strings       List of node pools to use for scheduling the job, ordered by preference.\n",
      "      --node-type string         Enforce node type affinity by setting a node-type label.\n",
      "      --toleration stringArray   toleration details formatted as: operator=Equal|Exists,key=KEY,[value=VALUE],[effect=NoSchedule|NoExecute|PreferNoSchedule],[seconds=SECONDS]\n",
      "\n",
      "Global Flags:\n",
      "      --loglevel string   Set the logging level. One of: debug|info|warn|error (default \"info\")\n",
      "  -p, --project string    Specify the project to which the command applies. By default, commands apply to the default project. To change the default project use ‘runai config project <project name>’.\n"
     ]
    }
   ],
   "source": [
    "!runai submit -h"
   ]
  },
